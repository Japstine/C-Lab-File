// e3 q2 part 1

#include <stdio.h>

int main() {

    printf("Name: Jeet Patel\nRoll Number: 23BIT156\n");
    
    int num1, num2, num3, num4, largest;

    printf("Enter four numbers: ");
    scanf("%d %d %d %d", &num1, &num2, &num3, &num4);

    if (num1 >= num2) {
        if (num1 >= num3) {
            if (num1 >= num4) {
                largest = num1;
            } else {
                largest = num4;
            }
        } else {
            if (num3 >= num4) {
                largest = num3;
            } else {
                largest = num4;
            }
        }
    } else {
        if (num2 >= num3) {
            if (num2 >= num4) {
                largest = num2;
            } else {
                largest = num4;
            }
        } else {
            if (num3 >= num4) {
                largest = num3;
            } else {
                largest = num4;
            }
        }
    }

    printf("The largest number is: %d\n", largest);

    return 0;
}


