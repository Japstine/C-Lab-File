// e5 q1 (Flexible Array)

#include <stdio.h>

int main() {
    printf("Name: Jeet Patel\nRoll Number: 23BIT156\n");

    int n;
    printf("Enter the size of the array: ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Invalid array size.\n");
        return 1;
    }

    int array[n];
    int positiveCount = 0;
    int negativeCount = 0;

    printf("Enter %d integers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &array[i]);
        if (array[i] > 0) {
            positiveCount++;
        } else if (array[i] < 0) {
            negativeCount++;
        }
    }

    printf("Number of positive numbers: %d\n", positiveCount);
    printf("Number of negative numbers: %d\n", negativeCount);

    return 0;
}

// e5 q2

#include <stdio.h>
#include <math.h>

int main() {
    printf("Name: Jeet Patel\nRoll Number: 23BIT156\n");

    int n = 10;
    double numbers[n];
    double sum = 0.0;
    double mean, variance, stdDev;

    // Input 10 numbers
    printf("Enter 10 numbers:\n");
    for (int i = 0; i < n; i++) {
        scanf("%lf", &numbers[i]);
        sum += numbers[i];
    }

    // Calculate mean
    mean = sum / n;

    // Calculate variance
    double varianceSum = 0.0;
    for (int i = 0; i < n; i++) {
        varianceSum += pow(numbers[i] - mean, 2);
    }
    variance = varianceSum / n;

    // Calculate standard deviation
    stdDev = sqrt(variance);

    // Display the results
    printf("Sum: %.2f\n", sum);
    printf("Mean: %.2f\n", mean);
    printf("Variance: %.2f\n", variance);
    printf("Standard Deviation: %.2f\n", stdDev);

    return 0;
}

// e5 q3

#include <stdio.h>

int main() {
    printf("Name: Jeet Patel\nRoll Number: 23BIT156\n");

    int rows, cols;

    printf("Enter the number of rows: ");
    scanf("%d", &rows);
    printf("Enter the number of columns: ");
    scanf("%d", &cols);

    int A[rows][cols], B[rows][cols], sum[rows][cols], difference[rows][cols];

    printf("Enter elements of matrix A:\n");
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            scanf("%d", &A[i][j]);
        }
    }

    printf("Enter elements of matrix B:\n");
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            scanf("%d", &B[i][j]);
        }
    }

    printf("A + B:\n");
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            sum[i][j] = A[i][j] + B[i][j];
            printf("%d ", sum[i][j]);
        }
        printf("\n");
    }

    printf("A - B:\n");
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            difference[i][j] = A[i][j] - B[i][j];
            printf("%d ", difference[i][j]);
        }
        printf("\n");
    }

    return 0;
}

