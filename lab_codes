    int sum = 0;
    int count = 0;

    printf("Integers between 0 and 100 divisible by 6 but not divisible by 4:\n");
    for (int i = 0; i <= 100; i++) {
        if (i % 6 == 0 && i % 4 != 0) {
            printf("%d ", i);
            sum += i;
            count++;
        }
    }

    printf("\nTotal count: %d\n", count);
    printf("Sum of the values: %d\n", sum);

    return 0;
}

